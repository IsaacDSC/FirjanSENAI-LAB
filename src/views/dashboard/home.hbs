<script>

    window.onload = () => {
        let DATERegister = document.querySelector('.dataRegister')

        let DT = new Date();

        dia = DT.getDate().toString(),
            diaF = (dia.length == 1) ? '0' + dia : dia
        mes = (DT.getMonth() + 1).toString()
        mesF = (mes.length == 1) ? '0' + mes : mes
        ano = DT.getUTCFullYear()

        DATERegister.innerHTML = ` ${diaF}/${mesF}/${ano}.`

    }
</script>
